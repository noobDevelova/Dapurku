<div
  class="modal fade"
  id="exampleModal"
  tabindex="-1"
  aria-labelledby="exampleModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="customModalLabel">
          <%= item.jenis_makanan %>
        </h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">
        <!-- Isi modal di sini -->
        <p>mmk</p>
      </div>
      <div class="modal-footer">
        <!-- Tombol atau elemen footer lainnya -->
        <p>mmk</p>
      </div>
    </div>
  </div>
</div>
